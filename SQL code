sqlite> SELECT * FROM walmart_sales LIMIT 10;

 SELECT SUM (weekly_sales) FROM walmart_sales;

SELECT store, MAX (weekly_sales) FROM walmart_sales;

sqlite> SELECT store, SUM (weekly_sales) FROM walmart_sales
   ...> GROUP BY store;

sqlite> SELECT store, MAX (total_weekly_sales) FROM
   ...> (SELECT store, SUM (weekly_sales) AS total_weekly_sales
(x1...> FROM walmart_sales GROUP BY store)
   ...> ;

sqlite> SELECT date, MAX (weekly_sales) FROM walmart_sales;
